{
  "buildCommand": "cd devprofile-frontend && npm install && npm run build",
  "outputDirectory": "devprofile-frontend/dist/devprofile-frontend",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://dev-profile-beige.vercel.app/api/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
